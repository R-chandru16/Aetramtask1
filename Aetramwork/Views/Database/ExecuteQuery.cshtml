
@model List<YourEntity>

<h2>Query Results for: @ViewBag.Query</h2>
<table border="1">
    <tr>
        <th>Id</th>
        <th>Name</th>
   
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.Id</td>
            <td>@item.Name</td>
       
        </tr>
    }
</table>
